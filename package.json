{
  "name": "brt-wallet",
  "version": "1.0.0",
  "productName": "BRT Wallet",
  "description": "BRT Desktop Wallet",
  "homepage": "https://github.com/BRTNetwork/brt-wallet",
  "author": {
    "name": "BRT Network",
    "email": "info@brt.org"
  },
  "keywords": [
    "brt",
    "brt",
    "wallet"
  ],
  "main": "electron.js",
  "private": true,
  "build": {
    "appId": "com.squirrel.brt-wallet.brt-wallet"
  },
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "npm-run-all --parallel webpack:watch electron:serve",
    "webpack:watch": "webpack --watch",
    "webpack:compile": "webpack --display-error-details",
    "compile-sass-theme": "sass src/assets/primeng-brt/theme.scss src/assets/primeng-brt/theme.css",
    "start:web": "webpack-dev-server --content-base . --port 4200 --inline",
    "build:electron:main": "tsc electron.ts --outDir dist && copyfiles package.json dist && cd dist && npm install --prod --legacy-peer-deps && cd ..",
    "build": "webpack --display-error-details && npm run build:electron:main",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "electron:serve": "npm run build:electron:main && electron ./dist --serve",
    "electron:test": "electron ./dist",
    "electron:dev": "npm run build && electron ./dist --debug",
    "electron:prod": "npm run build:prod && electron ./dist --debug",
    "electron:linux": "npm run build:prod && node package.js --asar --platform=linux --arch=x64",
    "electron:windows32": "npm run build:prod && node package.js --asar --platform=win32 --arch=ia32",
    "electron:windows64": "npm run build:prod && node package.js --asar --platform=win32 --arch=x64",
    "electron:mac": "npm run build:prod && node package.js --asar --platform=darwin --arch=x64",
    "electron:mac:sign": "electron-osx-sign ./app-builds/brt-wallet-darwin-x64/brt-wallet.app",
    "installer:windows32": "npm run electron:windows32 && node createWinInstaller.js --arch=ia32",
    "installer:windows64": "npm run electron:windows64 && node createWinInstaller.js --arch=x64",
    "installer:windows": "npm run installer:windows32 && npm run installer:windows64",
    "installer:mac": "npm run electron:mac && npm run electron:mac:sign && electron-installer-zip ./app-builds/brt-wallet-darwin-x64/brt-wallet.app release-builds/brt-wallet.zip && electron-installer-dmg ./app-builds/brt-wallet-darwin-x64/brt-wallet.app brt-wallet --out=release-builds --overwrite --icon=./src/assets/icons/brt.icns",
    "installer:linux": "npm run electron:linux && node createLinuxInstaller.js",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet && npm run build",
    "e2e": "protractor ./protractor.conf.js"
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/cdk": "5.2.5",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/material": "5.2.5",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@angular/router": "5.2.11",
    "@brtnetwork/brt-lib": "^1.0.12",
    "@types/node": "^10.17.55",
    "angular2-logger": "^0.7.0",
    "angular2-qrcode": "^2.0.1",
    "angular2-uuid": "1.1.1",
    "async": "^2.6.0",
    "big-integer": "^1.6.34",
    "big.js": "^5.0.3",
    "bufferutil": "^4.0.1",
    "core-js": "2.4.1",
    "electron-installer-debian": "^0.7.1",
    "electron-installer-dmg": "^0.2.1",
    "electron-installer-redhat": "^0.5.0",
    "electron-installer-zip": "^0.1.2",
    "electron-is-dev": "^0.3.0",
    "electron-linux-installer": "^0.2.2-beta",
    "electron-notification-desktop": "0.0.4",
    "electron-osx-sign": "^0.4.10",
    "electron-squirrel-startup": "^1.0.0",
    "enhanced-resolve": "3.3.0",
    "font-awesome": "^4.7.0",
    "fs-xattr": "^0.3.0",
    "fsync": "0.0.0",
    "lokijs": "1.5.5",
    "lz-string": "^1.4.4",
    "ngx-store": "1.4.3",
    "primeng": "5.2.7",
    "queueing-subject": "0.1.1",
    "reflect-metadata": "^0.1.12",
    "rmdir": "^1.2.0",
    "rxjs": "5.5.8",
    "rxjs-websockets": "4.0.0",
    "scandir": "^0.1.2",
    "utf-8-validate": "^5.0.2",
    "winston": "^2.4.0",
    "zone.js": "0.8.17"
  },
  "devDependencies": {
    "@angular-devkit/core": "^0.7.4",
    "@angular/cli": "1.6.0",
    "@angular/compiler-cli": "5.2.11",
    "@ngtools/webpack": "^1.10.2",
    "@types/jasmine": "^2.5.54",
    "autoprefixer": "7.1.4",
    "awesome-typescript-loader": "^5.2.0",
    "codelyzer": "3.2.0",
    "copyfiles": "1.2.0",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "css-to-string-loader": "^0.1.3",
    "cssnano": "3.10.0",
    "electron": "^5.0.13",
    "electron-builder": "^20.28.4",
    "electron-builder-squirrel-windows": "^20.28.3",
    "electron-packager": "^15.2.0",
    "electron-reload": "1.2.5",
    "electron-winstaller": "^2.6.4",
    "exports-loader": "0.6.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "0.11.2",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "https-proxy-agent": "2.2.1",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "json-loader": "0.5.7",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-sourcemap-loader": "0.3.7",
    "less-loader": "4.0.5",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.1",
    "postcss-loader": "2.0.6",
    "postcss-url": "7.1.2",
    "protractor": "5.1.2",
    "raw-loader": "0.5.1",
    "sass": "^1.32.8",
    "sass-loader": "6.0.6",
    "script-loader": "0.7.1",
    "source-map-loader": "0.2.3",
    "style-loader": "0.18.2",
    "stylus-loader": "3.0.1",
    "tar": "4.4.2",
    "ts-node": "3.3.0",
    "tslint": "5.7.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.9.9",
    "url-loader": "0.5.9",
    "webdriver-manager": "12.0.6",
    "webpack": "3.8.1",
    "webpack-concat-plugin": "^2.4.2",
    "webpack-dev-server": "2.9.2"
  },
  "optionalDependencies": {
    "electron-installer-debian": "^0.7.1",
    "electron-installer-dmg": "^0.2.1",
    "electron-installer-redhat": "^0.5.0",
    "electron-installer-zip": "^0.1.2",
    "electron-linux-installer": "^0.2.2-beta"
  },
  "license": "SEE LICENSE IN LICENSE.md"
}
